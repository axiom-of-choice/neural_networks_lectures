      "source": [
        "def mapa_colores(y):\n",
        "    y = np.array(y)\n",
        "    y = y.ravel()\n",
        "    mc = []\n",
        "    for i in y:\n",
        "        if i == 0:\n",
        "            mc.append(\"blue\")\n",
        "        else:\n",
        "            mc.append(\"red\")\n",
        "    return mc "
      ],
