        "id": "eiVVyDCLfNwz"
      },
      "source": [
        "def predicciones(X,w):\n",
        "    pred = sigmoide(X@w)\n",
        "    pred = pred.ravel()\n",
        "    etiquetas = []\n",
        "    for i in pred:\n",
        "        if i>0.5:\n",
        "            etiquetas.append(1)\n",
        "        else:\n",
        "            etiquetas.append(0)\n",
        "    return etiquetas\n",
        "    "
      ],
      "execution_count": null,
      "outputs": []
    },
